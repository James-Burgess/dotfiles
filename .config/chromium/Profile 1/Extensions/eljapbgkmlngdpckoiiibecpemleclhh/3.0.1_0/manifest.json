{
   "author": "Creaktif",
   "background": {
      "persistent": false,
      "scripts": [ "app.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icons/19x19.png",
         "38": "icons/38x38.png"
      },
      "default_title": "Fontface Ninja"
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "Explore fonts within a website, try, bookmark and buy them !",
   "homepage_url": "https://www.fontface.ninja",
   "icons": {
      "128": "icons/128x128.png",
      "16": "icons/16x16.png",
      "48": "icons/48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Fontface Ninja",
   "permissions": [ "http://*/", "https://*/", "tabs", "activeTab", "storage", "cookies" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.1",
   "web_accessible_resources": [ "/inject.css", "/inject.js" ]
}
